{
  "name": "No Destractions",
  "manifest_version": 3,
  "version": "1.0.0",
  "description": "Allows you to limit the amount of time you can access sites",
  "content_scripts": [{
    "js": [ "content.js"],
    "matches": [ "<all_urls>"]
   }],
  "background": {
    "service_worker": "background.js"
   },
   "permissions": ["tabs", "nativeMessaging"]
}