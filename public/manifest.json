{
  "name": "No Destractions",
  "manifest_version": 2,
  "version": "0.0.1",
  "description": "Allows you to limit the amount of time you can access sites",
  "content_scripts": [{
    "js": [ "content.js"],
    "matches": [ "<all_urls>"]
   }],
  "background": {
    "scripts": ["background.js"]
   },
   "permissions": ["contextMenus", "tabs"],
   "web_accessible_resources" : ["*.html"],
   "content_security_policy": "script-src 'self' 'sha256-ILo8Sg5ameOh7EJPaZmIQYqDkAozAPY56yCIVu/8hPo='; object-src 'self'"
}
